<h1> New Student Account</h1>

<%= form_tag "/students", method: :post do %>
<div>
First Name: <input type="text" name="first_name" value="<%= @student.first_name %>">
</div>

<div>
Last Name: <input type="integer" name="last_name" value="<%= @student.last_name %>">
</div>

<div>
email: <input type="text" name="email" value="<%= @student.email %>">
</div>

<div>
password: <input type="text" name="password" value="<%= @student.password %>">
</div>

<input type="submit" value="Create New Student Account">
<% end %>
