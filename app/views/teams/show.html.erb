<h2><%= @project.name %> Project</h2>
<br>

<div class="row space-bottom-2x">
  <div class="col-sm-6">
    <h4><%= @team.name %> Group Members</h4>
      <div class="row">
        <div class="col-sm-12">
          <div class="well">
            <% @students.each do |student| %>
              <p><%= student.name %></p> 
            <% end %> 
          </div> <!-- / well -->
        </div><!-- / col-sm-12 -->
      </div><!-- / row -->
    <br> 
  </div>
  <div class="col-sm-6">
    <h4> Tasks </h4>
    <% @tasks.each do |task| %>
      <div class="row">
        <div class="col-sm-12">
          <div class="well">
            <p>due date: <%= task.deadline %></p>
            <p>points: <%= task.points %> points</p>
            <p>description:<%= task.description %></p>
            <br> 
          </div>   <!-- / well -->
        </div><!-- / col-sm-12 -->
      </div><!-- / row -->
    <% end %> 
  </div>
</div><!-- / row -->
<!-- / 2col text blocks -->